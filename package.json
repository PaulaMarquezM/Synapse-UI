{
  "name": "synapse-ui",
  "displayName": "Synapse ui",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Pau",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "@supabase/supabase-js": "^2.91.0",
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@vladmandic/face-api": "^1.7.15",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.26.2",
    "lucide-react": "^0.562.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "camera",
      "storage"
    ],
    "host_permissions": [
      "https://*/*"
    ],
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "web_accessible_resources": [
      {
        "resources": [
          "assets/models/*",
          "assets/models/**/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
