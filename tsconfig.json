{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules",
    "build",
    ".plasmo"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "jsx": "react-jsx",
    "lib": ["dom", "es2021"],
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "~*": ["./src/*"]
    }
  }
}
